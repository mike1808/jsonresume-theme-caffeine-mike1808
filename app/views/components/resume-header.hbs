{{#resume.basics}}
<header class="resume-header">
    <div class="profile-header">
        <h1>{{name}}</h1>
        <h2>{{label}}</h2>
    </div>
    <div class="profile-pic">
        {{#if picture}}
        <img src="{{picture}}" alt="profile-pic">
        {{/if}}
    </div>
</header>
{{/resume.basics}}
